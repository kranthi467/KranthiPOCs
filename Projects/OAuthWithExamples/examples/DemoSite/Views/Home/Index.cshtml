@{
  ViewBag.Title = DemoOAuth.Strings.DemoTitle;
  Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Request.IsLocal)
{
  @Html.Raw(DemoOAuth.Strings.NoForLocalhost)
}

<h1>@DemoOAuth.Strings.DemoTitle</h1>

@Html.Raw(DemoOAuth.Strings.DemoIntro)

@* !!!USE JAVASCRIPT LINKS TO PREVENT THEM FROM INDEXING!!! *@
<ul style="width:600px;margin-left:auto;margin-right:auto;" class="oauth">
  @foreach (var client in Nemiro.OAuth.OAuthManager.RegisteredClients)
  {
    <li>
      @*link and image*@
      <a href="#" onclick="window.location.href='@Url.Action("ExternalLogin", new { provider = client.Key })'; return false;">
        <img src="@Url.Action("Icon", new { id = client.Key })/" alt="@client.Key" title="@client.Key" width="100" height="100" />
      </a>
    </li>
  }
	@if ((Nemiro.OAuth.OAuthManager.RegisteredClients.Count % 5) != 0)
	{
		for (int i = 0; i <= 5 - (Nemiro.OAuth.OAuthManager.RegisteredClients.Count % 5) - 1; i++)
		{
			<li><img src="@Url.Action("Icon", new { id = "empty" })" alt="empty" title="" width="100" height="100" /></li>
		}
	}
</ul>

<div class="clear"></div>

@Html.Raw(DemoOAuth.Strings.DemoFeedback)
<hr />

@Html.Raw(String.Format(DemoOAuth.Strings.DemoTechInfo, System.Reflection.Assembly.GetAssembly(typeof(Nemiro.OAuth.OAuthWeb)).GetName().Version, ViewBag.Version))

